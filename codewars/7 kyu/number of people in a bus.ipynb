    "def number(bus_stops):\n",
    "    e = [x for t in bus_stops for x in t]\n",
    "    f = e[1::2]\n",
    "    g = e[0::2]\n",
    "    sub = sum(f)\n",
    "    su = sum(g)\n",
    "    x = su - sub\n",
    "    return x"
   ]
  }
