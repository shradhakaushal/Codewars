    "import math\n",
    "def predict_age(a1, a2, a3, a4, a5, a6, a7, a8):\n",
    "    x = a1*a1 + a2*a2 + a3*a3 + a4*a4 + a5*a5 + a6*a6 + a7*a7 + a8*a8\n",
    "    y = math.sqrt(x)\n",
    "    d = y//2\n",
    "    return d\n"
