    "def L(n, l0, l1, add):\n",
    "    count = 0\n",
    "    LD = [l0 , l1]\n",
    "    leo = []\n",
    "    while count < n:\n",
    "        ln = l0 + l1 + add\n",
    "        l0 = l1\n",
    "        l1 = ln\n",
    "        count +=1\n",
    "        LD.append(ln)\n",
    "        elm = LD.pop(0)\n",
    "        leo.append(elm)\n",
    "    return (leo)"
   ]
  }
