    "def is_prime(num):\n",
    "    if num == 1:\n",
    "        return False\n",
    "    for i in range(2, num):\n",
    "        if num%i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "            \n",
    "def next_prime(num):\n",
    "    while True:\n",
    "        num += 1\n",
    "        if is_prime(num):\n",
    "            return num"
